{
  "name": "@tummycrypt/tinyland-video-thumbnails",
  "version": "0.1.0",
  "description": "Video thumbnail resolver for YouTube, PeerTube, and Vimeo with in-memory caching",
  "type": "module",
  "license": "Zlib AND LicenseRef-Tinyland-Proprietary",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "video",
    "thumbnail",
    "youtube",
    "peertube",
    "vimeo"
  ],
  "author": "Tinyland.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinyland-inc/tinyland.dev",
    "directory": "packages/tinyland-video-thumbnails"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true
  }
}
